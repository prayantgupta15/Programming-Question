#include <iostream>
#include<math.h>
#include<sstream>
#include<string>
using namespace std;

int main() {
	//code
    int t=0;
    while(t<1||t>16)
    cin>>t;
    while(t--){
        
     int sum =0;   
    string str;
    cin>>str;
    
    for(int i=0;i<str.length();i++)
    {   stringstream s;
        s<<str[i];
        int n;
        s >>n;
        sum = sum + n*pow(2,str.length()-i-1);
    }
    cout<<sum<<"\n";
    }
	return 0;
}
